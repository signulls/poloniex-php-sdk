{
    "name": "signulls/poloniex-php-sdk",
    "description": "Poloniex REST API Client",
    "homepage": "http://github.com/signulls/poloniex-php-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Grisha Chasovskih",
            "email": "chasovskihgrisha@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/signulls/poloniex-php-sdk/issues"
    },
    "keywords": [
        "poloniex",
        "php"
    ],
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "psr/log": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/serializer": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Poloniex\\": "src/"
        }
    }
}
